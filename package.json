{
	"name": "vite-best-template",
	"version": "0.0.0",
	"scripts": {
		"dev": "cross-env NODE_ENV=development vite --mode dev",
		"build": "cross-env NODE_ENV=production vue-tsc --noEmit && vite build --mode prod",
		"serve": "vite preview",
		"gen-services": "npx swagger2js gen ./swagger-serveces-api.json"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"mockjs": "^1.1.0",
		"vue": "^3.0.5",
		"vue-router": "^4.0.5",
		"vuex": "^4.0.0"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^4.20.0",
		"@typescript-eslint/parser": "^4.20.0",
		"@vitejs/plugin-vue": "^1.1.5",
		"@vue/compiler-sfc": "^3.0.5",
		"cross-env": "^7.0.3",
		"eslint": "^7.23.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-vue": "^7.8.0",
		"husky": "^6.0.0",
		"lint-staged": "^10.5.4",
		"path": "^0.12.7",
		"prettier": "^2.2.1",
		"sass": "^1.32.8",
		"swagger-js-codegen-purvar": "^1.21.1",
		"typescript": "^4.1.3",
		"vite": "^2.1.3",
		"vite-plugin-mock": "^2.4.0",
		"vue-tsc": "^0.0.8"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"src/**/*.{ts,vue}": [
			"prettier --write",
			"eslint --cache --fix",
			"git add"
		]
	}
}
